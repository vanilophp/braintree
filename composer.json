{
    "name": "vanilo/braintree",
    "description": "Braintree Payment Gateway Support for Vanilo (Laravel)",
    "type": "library",
    "keywords": ["laravel", "vanilo", "braintree", "payments"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Hunor Kedves"
        },
        {
            "name": "Attila Fulop"
        }
    ],
    "require": {
        "php": "^8.2",
        "braintree/braintree_php": "^6.11.1",
        "illuminate/contracts": "^10.0|^11.0",
        "konekt/concord": "^1.13",
        "vanilo/payment": "^4.0",
        "vanilo/contracts": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Vanilo\\Braintree\\": "src"
        }
    },
    "require-dev": {
        "ext-sqlite3": "*",
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit" : "^10.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Braintree\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
